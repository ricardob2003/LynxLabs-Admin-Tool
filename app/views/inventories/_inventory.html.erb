<div id="<%= dom_id inventory %>">
  <p>
    <strong>Nombre:</strong>
    <%= inventory.name %>
  </p>
  <p>
    <strong>Modelo:</strong>
    <%= inventory.model %>
  </p>
  <p>
    <strong>Numero de Serie:</strong>
    <%= inventory.serial_number %>
  </p>
  <p>
    <strong>Estado del Equipo:</strong>
    <%= inventory.status %>
  </p>
  <p>
    <strong>Fecha de Compra:</strong>
    <%= inventory.entry_date %>
  </p>
  <p>
    <strong>ID del Equipo:</strong>
    <%= inventory.lynx_id_number %>
  </p>
  <p>
    <strong>Precio de Compra :</strong>
    <%= inventory.purchase_price %>
  </p>
  <p>
    <strong>Costo Pro-Rateado:</strong>
    <%= inventory.current_cost %>
  </p>
  <p>
    <strong>Vencimiento de Garantia:</strong>
    <%= inventory.warranty_expiration %>
  </p>
</div>
